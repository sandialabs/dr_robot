    "Enumeration" :
    {
        "HTTPScreenshot": {
            "name" : "HTTPScreenshot",
            "short_name" : "http",
            "default" : 2,
            "docker_name" : "httpscreen",
            "default_conf" : "dockers/Dockerfile.HTTPScreenshot.tmp",
            "active_conf" : "dockers/Dockerfile.HTTPScreenshot",
            "ansible_arguments" : {
                "config" : "$config/ansible_plays/httpscreenshot_play.yml",
                "flags": "-e '$extra'",
                "extra_flags":{
                    "1" : "variable_host=localhost",
                    "2" : "infile=$infile/aggregated_hostnames.txt",
                    "3" : "outfile=$outfile/httpscreenshots.tar",
                    "4" : "outfolder=$outfile/httpscreenshots"
                }
            },
            "description" : "Post enumeration tool for screen grabbing websites. All images will be downloaded to outfile: httpscreenshot.tar and unpacked httpscreenshots",
            "output" : "/tmp/output",
            "inputfilelist" : "aggregated_hostnames.txt",
            "enabled" : false
        },
